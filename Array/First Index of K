import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
//Print only the first index of k in the array.
	   Scanner sc = new Scanner (System.in);
	   int n =sc.nextInt();
	   int k = sc.nextInt();
	   int [] arr = new int[n];
	   for(int i= 0;i<n;i++){
	       arr[i] = sc.nextInt();
	   }
	   for (int i=0; i<n;i++){
	       if(arr[i]==k){
	           System.out.println(i);
	           //here is the only difference (we use break)
	           break;
	       }
	   }
	}
}
