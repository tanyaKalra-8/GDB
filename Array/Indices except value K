import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	   //Print all the indices except the one having the value k.
	   Scanner sc = new Scanner (System.in);
	   int n =sc.nextInt();
	   int k = sc.nextInt();
	   int [] arr = new int[n];
	   for(int i= 0;i<n;i++){
	       arr[i] = sc.nextInt();
	   }
	   for (int i=0; i<n;i++){
	       //if(arr[i]!=k){
	       if(arr[i]==k){
	           System.out.println(i);
	           continue;
	       }
	   }
	}
}
